DISABLE_CACHE=false
# Showbox proxy configuration
# Primary proxy URL with the format ending in /?destination=
SHOWBOX_PROXY_URL_VALUE=https://starlit-valkyrie-39f5ab.netlify.app/?destination=
# Alternative proxy URL for rotation (same format)
SHOWBOX_PROXY_URL_ALTERNATE=https://frolicking-semolina-769185.netlify.app/?destination=
# Set to true to enable proxy rotation between primary and alternate URLs
SHOWBOX_USE_ROTATING_PROXY=true
# SCRAPER_MODE=proxy
# SCRAPER_API_KEY_VALUE=your_scraper_api_key
# USE_SCRAPER_API=false
TMDB_API_KEY=
# For Xprime, use the direct proxy URL without any suffix (no /?destination= needed)
# Example: XPRIME_PROXY_URL=https://nuvioaddon.duckdns.org
XPRIME_PROXY_URL=
XPRIME_USE_PROXY=false
# Set to true to enable Cuevana. Often requires a proxy configured for the addon or Showbox proxy if it's routed through there.
