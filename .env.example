ENABLE_HOLLYMOVIEHD_PROVIDER=false
# Set to false to disable the Xprime.tv provider
ENABLE_XPRIME_PROVIDER=true
# (Optional) URL for your self-hosted Hianime scraper service (see README for setup details)
